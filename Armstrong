package reshma;
import java.util.Scanner;
import java.lang.*;
public class Armstrong {
	public static void main(String[] args)
	{
		Scanner input=new Scanner(System.in); 
		int n=input.nextInt(); 
		int num=n; 
		int sum=0; 
		while(num!=0) 
		{ 
		    int rem=num%10; 
		    sum=sum+(rem*rem*rem); 
		    num=num/10; 
		} 
		if(n==sum) 
		    System.out.print("Armstrong number"); 
		else 
		    System.out.print("Not Armstrong"); 

	}

}
