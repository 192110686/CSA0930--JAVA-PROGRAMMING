public class Pair<K, V> {
    private K key;
    private V value;

    public Pair(K key, V value) {
        this.key = key;
        this.value = value;
    }

    public K getKey() {
        return key;
    }

    public V getValue() {
        return value;
    }

    public static void main(String[] args) {
        // Create a Pair of Integer and String
        Pair<Integer, String> pair = new Pair<>(1, "One");
        System.out.println("Key: " + pair.getKey());
        System.out.println("Value: " + pair.getValue());
        
        // Create a Pair of String and Double
        Pair<String, Double> anotherPair = new Pair<>("PI", 3.14);
        System.out.println("Key: " + anotherPair.getKey());
        System.out.println("Value: " + anotherPair.getValue());
    }
}
