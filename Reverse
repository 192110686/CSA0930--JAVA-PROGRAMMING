package reshma;
import java.util.Scanner;
import java.lang.*;
public class Reverse {
	public static void main(String args[])
	{
		Scanner in=new Scanner(System.in); 
		System.out.println("enter the string to reverse:");
		String s=in.nextLine(); 
		String b=""; 
		int len=s.length(); 
		for(int i=len-1;i>=0;i--) 
		{ 
		b=b+s.charAt(i); 
		} 
		System.out.print(b); 
	}

}
