import java.util.Random;

public class LuckyGuess {
    public static void main(String[] args) {
        int randomNumber = generateRandomNumber();
        int luckyNumber = 7; // Assuming lucky number is 7

        switch (randomNumber) {
            case 1:
                System.out.println("Sorry, " + randomNumber + " is not the lucky guess.");
                break;
            case 2:
                System.out.println("Sorry, " + randomNumber + " is not the lucky guess.");
                break;
            case 3:
                System.out.println("Sorry, " + randomNumber + " is not the lucky guess.");
                break;
            case 4:
                System.out.println("Sorry, " + randomNumber + " is not the lucky guess.");
                break;
            case 5:
                System.out.println("Sorry, " + randomNumber + " is not the lucky guess.");
                break;
            case 6:
                System.out.println("Sorry, " + randomNumber + " is not the lucky guess.");
                break;
            case 7:
                System.out.println("Congratulations! " + randomNumber + " is the lucky guess!");
                break;
            default:
                System.out.println("Sorry, " + randomNumber + " is not the lucky guess.");
        }
    }

    public static int generateRandomNumber() {
        Random random = new Random();
        return random.nextInt(10) + 1; // Generate a random number between 1 and 10
    }
}
